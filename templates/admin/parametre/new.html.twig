{% extends 'admin/layout/base.html.twig' %}

{% block body %}
<div class="container">
    <form method="post">
        <div class="row mb-3">
            <label>Facebook</label>
            <input type="text" class="form-control" value="{{ facebook|default('') }}" name="FACEBOOK">
        </div>
        <div class="row mb-3">
            <label>INSTAGRAM</label>
            <input type="text" class="form-control" value="{{ instagram|default('') }}" name="INSTAGRAM">
        </div>
        <div class="row mb-3">
            <label>YOUTUBE</label>
            <input type="text" class="form-control" value="{{ youtube|default('') }}" name="YOUTUBE">
        </div>
        <div class="row mb-3">
            <label>LINKEDIN</label>
            <input type="text" class="form-control" value="{{ linkedin|default('') }}" name="LINKEDIN">
