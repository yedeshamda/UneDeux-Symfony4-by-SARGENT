{% extends 'admin/layout/base.html.twig' %}
{% block body %}
    <h1>Détails du Categorie</h1>

    <table class="table">
        <tbody>
        <tr>
            <th>Nom</th>
            <td>{{ contact.nom }}</td>
        </tr>
        <tr>
            <th>Prenom</th>
            <td>{{ contact.prenom }}</td>
        </tr>
        <tr>
            <th>Tel</th>
            <td>{{ contact.tel }}</td>
        </tr>
        <tr>
            <th>Email</th>
            <td>{{ contact.email }}</td>
        </tr>
        <tr>
            <th>Message</th>
            <td>{{ contact.message }}</td>
        </tr>
        </tbody>
    </table>
    <center>
    {{ include('admin/contact/_delete_form.html.twig') }}
    <br>
    </center>
    <br>
    <a href="{{ path('admin_contact_index') }}">Précédent</a>
    <br>

{% endblock %}
